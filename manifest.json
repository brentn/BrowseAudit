{
  "name": "BrowseAudit",
  "description": "Summarize browsing history",
  "version": "0.1",
  "manifest_version":2,
  "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
  },
  "background": {
    "scripts": [
        "libs/jquery.min.js",
        "src/background.js"
    ],
    "persistent": true
  },
  "content_scripts": [
      {
          "js": [
            "src/activity.js"
          ],
          "matches": ["*://*/*"]
      }
  ],
  "permissions": [
    "tabs",
    "storage"
  ]
}
